{
  "name": "claude",
  "command": "claude",
  "additional_args": [
    "--permission-mode",
    "acceptEdits",
    "--model",
    "sonnet"
  ],
  "env": {},
  "max_retries": 5,
  "retry_delays": [3.0, 6.0, 12.0, 24.0, 48.0],
  "roles": {
    "default": {
      "prompt_path": "systemprompts/clink/default.txt",
      "role_args": []
    },
    "planner": {
      "prompt_path": "systemprompts/clink/default_planner.txt",
      "role_args": []
    },
    "codereviewer": {
      "prompt_path": "systemprompts/clink/default_codereviewer.txt",
      "role_args": []
    }
  }
}
